          "type": "list",
          "settings": {
            "list_title": "Courses and Certifications",
            "list_text_1": "<li>Level 1 & 2 Concussion Management Workshop<\/li><li>Level 1 Soft Tissue Release Workshop<\/li>\n<li>Level 1 Manual Therapy, Canadian Academy of Manipulative Therapy<\/li>"
          }
        }
